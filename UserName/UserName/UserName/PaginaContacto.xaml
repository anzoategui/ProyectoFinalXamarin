<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UserName.PaginaContacto" Padding="10" Title="Contacto"
             BackgroundColor="White">
    <StackLayout>
        <Label Text="{Binding Nombre}" Font="Large" TextColor="Black"/>
        <Label Text="{Binding Telefono}" Font="Small" TextColor="Black"/>
        <Label Text="{Binding Direccion}" Font="Small" TextColor="Black"/>
    </StackLayout>

</ContentPage>